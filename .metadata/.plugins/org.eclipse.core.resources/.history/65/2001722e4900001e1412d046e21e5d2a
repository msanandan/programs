package com.patteranprogram;

public class RevareLogic {
	
	public static void main(String[] args) {
		
		
		int(int i=1; i<=5; i++)
		
		{
			System.out.println();
			
			for(int j=5; j>i; j--)
			{
				System.out.println(" ");
			}
			
			for(int j  =1;j<=i;j++)
				
				
			{
				System.out.println(j+"");
			}
		}
	}

}
